set(PACKAGE_VERSION @META_REFL_VERSION@)

if("${PACKAGE_FIND_VERSION_MAJOR}" EQUAL "@META_REFL_VERSION_MAJOR@")
   if("${PACKAGE_FIND_VERSION_MINOR}" EQUAL "@META_REFL_VERSION_MINOR@")
      set(PACKAGE_VERSION_MATCH TRUE)
   elseif("${PACKAGE_FIND_VERSION_MINOR}" LESS "@META_REFL_VERSION_MINOR@")
      set(PACKAGE_VERSION_COMPATIBLE TRUE)
   else()
      set(PACKAGE_VERSION_UNSUITABLE TRUE)
   endif()
else()
   set(PACKAGE_VERSION_UNSUITABLE TRUE)
endif()
